							"pm.test(\"First team results are correct\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData[0].teamNumber).to.equal(2);\r",
							"    pm.expect(jsonData[0].teamName).to.equal(\"Team2\");\r",
							"    pm.expect(jsonData[0].totalValue).to.equal(24);\r",
							"    pm.expect(jsonData[0].numberOfPoints).to.equal(2);\r",
							"    pm.expect(jsonData[0].numberOfStages).to.equal(2);\r",
							"});\r",
